{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "require-jsdoc":"off",
        "no-unused-vars": "off"
    },
    "settings": {
        "import/resolver": {
            "typescript": {}
        }
    }
}
